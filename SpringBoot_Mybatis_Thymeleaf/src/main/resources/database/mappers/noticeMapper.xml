<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.ee.y1.board.notice.NoticeMapper">
  
  	<select id="getList" resultType="NoticeVO">
  		select * from notice order by num desc
  	</select>
  	
  	<select id="getSelect" parameterType="BoardVO" resultType="NoticeVO">
  		select * from notice where num = #{num}
  	</select>
  	
  	<insert id="setInsert" parameterType="BoardVO">
   			insert into notice values(#{num}, #{title}, #{writer},#{contents}, now(), 0)
   	</insert>
   
  
  </mapper>